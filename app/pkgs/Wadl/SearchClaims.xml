      <string name="ManualTriggerInput_contractholdername_Value_def" />
      <string name="ManualTriggerInput_contractholderid_Value_def" />
      <string name="ManualTriggerInput_contractbeneficiaryname_Value_def" />
      <string name="ManualTriggerInput_contractbeneficiaryid_Value_def" />
      <string name="ManualTriggerInput_contracttypename_Value_def" />
      <string name="ManualTriggerInput_contracttypedepartment_Value_def" />
      <string name="ManualTriggerInput_created_Value_def" />
      <object name="ManualTriggerInput">
        <property name="number_Value" type="string" />
        <property name="contractnumber_Value" type="string" />
        <property name="contractholdername_Value" type="string" />
        <property name="contractholderid_Value" type="string" />
        <property name="contractbeneficiaryname_Value" type="string" />
        <property name="contractbeneficiaryid_Value" type="string" />
        <property name="contracttypename_Value" type="string" />
        <property name="contracttypedepartment_Value" type="string" />
        <property name="created_Value" type="string" />
      </object>
      <string name="ResponseActionOutput_items_def_created_def" />
      <string name="ResponseActionOutput_items_def_number_def" />
      <string name="ResponseActionOutput_items_def_contracts_def_href_def" />
