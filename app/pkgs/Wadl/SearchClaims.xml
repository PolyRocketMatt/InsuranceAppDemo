  <resources siena:authenticationProviderHref="#PowerAppAuth" base="https://europe-002.azure-apim.net/apim/logicflows">
    <resource path="/{connectionId}/triggers/manual/run">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="POST" id="Run">
        <doc title="Runs a logic flow trigger." />
        <request>
          <param style="query" name="api-version" type="xs:string" required="true" fixed="2015-02-01-preview">
            <doc title="The api version" />
          </param>
          <representation mediaType="application/json">
            <param style="plain" name="number_Value" path="/number_Value" type="xs:string" required="true" />
            <param style="plain" name="contractnumber_Value" path="/contractnumber_Value" type="xs:string" required="true" />
            <param style="plain" name="contractholdername_Value" path="/contractholdername_Value" type="xs:string" required="true" />
            <param style="plain" name="contractholderid_Value" path="/contractholderid_Value" type="xs:string" required="true" />
            <param style="plain" name="contractbeneficiaryname_Value" path="/contractbeneficiaryname_Value" type="xs:string" required="true" />
            <param style="plain" name="contractbeneficiaryid_Value" path="/contractbeneficiaryid_Value" type="xs:string" required="true" />
            <param style="plain" name="contracttypename_Value" path="/contracttypename_Value" type="xs:string" required="true" />
            <param style="plain" name="contracttypedepartment_Value" path="/contracttypedepartment_Value" type="xs:string" required="true" />
            <param style="plain" name="created_Value" path="/created_Value" type="xs:string" required="true" />
          </representation>
        </request>
        <response>
          <representation mediaType="application/json" element="service:ResponseActionOutput" />
        </response>
      </method>
    </resource>
  </resources>
  <authenticationProviders xmlns="http://schemas.microsoft.com/MicrosoftProjectSiena/WADL/2014/11">
    <connectionProvider id="PowerAppAuth" siena:connectionProviderId="logicflows" />
  </authenticationProviders>
  <template xmlns="http://schemas.microsoft.com/MicrosoftProjectSiena/WADL/2014/11">
    <variable name="connectionId">
      <modifyParams style="template" name="connectionId" attribute="fixed" />
    </variable>
  </template>
